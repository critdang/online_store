// research set up rule https://eslint.org/docs/latest/rules/no-unused-vars
// npm install eslint --save-dev
// npx eslint --init
{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": [
      "error",
      { "args": "none", "varsIgnorePattern": "seed" }
    ],
    "linebreak-style": 0,
    "no-unused-expressions": "off",
    "max-len": ["error", { "code": 200 }],
    "sort-imports": ["error"],
    "consistent-return": "off"
  }
}
